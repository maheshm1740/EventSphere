                  },
                  "AvailableSeats": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "CategoryId": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "Image": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "Title": {
                  "style": "form"
                },
                "Description": {
                  "style": "form"
                },
                "StartDate": {
                  "style": "form"
                },
                "EndDate": {
                  "style": "form"
                },
                "Location": {
                  "style": "form"
                },
                "BuildingName": {
                  "style": "form"
                },
                "BuildingNumber": {
                  "style": "form"
                },
                "AvailableSeats": {
                  "style": "form"
                },
                "CategoryId": {
                  "style": "form"
                },
                "Image": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
